<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane maxHeight="-Infinity" maxWidth="-Infinity" minWidth="300.0" stylesheets="@alert.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.example.PrinterJob.PrintBorderless.CAlertPrintProgress">
    <header>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="26.0" style="-fx-background-color: #2369B5;">
         <children>
            <HBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label fx:id="labelHeader" text="Header" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children></AnchorPane>
    </header>
    <content>
      <StackPane>
         <children>
            <AnchorPane fx:id="anchorPaneContent" minHeight="0.0" minWidth="0.0">
               <children>
                  <BorderPane minHeight="60.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <left>
                        <AnchorPane BorderPane.alignment="CENTER" />
                     </left>
                     <center>
                        <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
                           <children>
                              <ProgressBar fx:id="progressBar" prefWidth="200.0" />
                           </children>
                        </VBox>
                     </center>
                  </BorderPane>
               </children>
            </AnchorPane>
         </children>
      </StackPane>
    </content>
</DialogPane>
